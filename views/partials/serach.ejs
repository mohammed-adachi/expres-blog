<div>
<h1>chercher</h1>

</div>
<section class="articles">
  <h2 class="articles__heading">Latest Posts</h2>

  <ul class="article-ul">
    
    <% data.forEach(post => { %>
      <li>
          <a href="/post/<%= post._id %>"> <!-- Assurez-vous que vous avez un champ _id dans vos données -->
              <span><%- post.title %></span>

              <span class="article-list__date"><%- post.createdAt.toDateString() %></span> <!-- Affichez la date de création de l'article -->
          </a>
          
      </li>
  <% }) %>
   
  </ul>


  
 

  



</section>